<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

  <Button android:layout_width="wrap_content" android:text="start"
      android:layout_height="wrap_content"
      android:id="@+id/start"/>
  <Button android:layout_width="wrap_content" android:text="stop"
      android:layout_height="wrap_content"
      android:id="@+id/stop"/>
  <Button android:layout_width="wrap_content" android:text="get"
      android:layout_height="wrap_content"
      android:id="@+id/remote_call_get"/>
  <Button android:layout_width="wrap_content" android:text="set"
      android:layout_height="wrap_content"
      android:id="@+id/remote_call_set"/>
  <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
      android:id="@+id/result" android:text="result"/>

</LinearLayout>
